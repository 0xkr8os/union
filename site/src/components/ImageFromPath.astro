---
import { Image, ImgAttributes } from 'astro:assets'

interface Props extends ImgAttributes {
  src: string
  alt: string
  height?: string
  width?: string
  layout?: 'responsive' | 'fixed'
}

const { src, alt, height = 'auto', width = '100%', layout } = Astro.props
---

<Image src={src} alt={'alt'} height={height.toString()} width={width} />
